  );\r\n};\r\n\r\nPagination.propTypes = {\r\n    itemsCount: PropTypes.number.isRequired,\r\n    pageSize: PropTypes.number.isRequired,\r\n    currentPage: PropTypes.number.isRequired,\r\n    onPageChange: PropTypes.func.isRequired,\r\n};\r\n\r\nexport default Pagination;\r\n",["81","82"],"C:\\Users\\trieu\\Documents\\fsa\\fsa-ui-dev\\src\\utils\\paginate.js",[],"C:\\Users\\trieu\\Documents\\fsa\\fsa-ui-dev\\src\\services\\companyService.js",[],"C:\\Users\\trieu\\Documents\\fsa\\fsa-ui-dev\\src\\services\\factorService.js",[],"C:\\Users\\trieu\\Documents\\fsa\\fsa-ui-dev\\src\\components\\aboutUs.jsx",[],"C:\\Users\\trieu\\Documents\\fsa\\fsa-ui-dev\\src\\components\\common\\profile.jsx",[],{"ruleId":"83","severity":1,"message":"84","line":2,"column":23,"nodeType":"85","messageId":"86","endLine":2,"endColumn":34},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","severity":1,"message":"92","line":90,"column":25,"nodeType":"93","endLine":90,"endColumn":53},{"ruleId":"91","severity":1,"message":"92","line":95,"column":25,"nodeType":"93","endLine":95,"endColumn":51},{"ruleId":"87","replacedBy":"94"},{"ruleId":"89","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":20,"column":62,"nodeType":"98","messageId":"99","endLine":20,"endColumn":64},{"ruleId":"96","severity":1,"message":"97","line":26,"column":49,"nodeType":"98","messageId":"99","endLine":26,"endColumn":51},{"ruleId":"100","severity":1,"message":"101","line":42,"column":25,"nodeType":"93","endLine":45,"endColumn":26},{"ruleId":"87","replacedBy":"102"},{"ruleId":"89","replacedBy":"103"},"no-unused-vars","'NavDropdown' is defined but never used.","Identifier","unusedVar","no-native-reassign",["104"],"no-negated-in-lhs",["105"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement",["104"],["105"],"eqeqeq","Expected '!==' and instead saw '!='.","BinaryExpression","unexpec